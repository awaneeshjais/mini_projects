<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.hello"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="19"/>
    <application android:label="@string/app_name" android:icon="@drawable/fx_top_page">
        <activity android:name="MyActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name="com.example.hello.PageBradis"
                android:label="@string/app_name"
                android:parentActivityName="com.example.hello.MyActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.hello.MyActivity" />
        </activity>

        <activity
                android:name="com.example.hello.PageHistory"
                android:label="@string/app_name"
                android:parentActivityName="com.example.hello.MyActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.hello.MyActivity" />
        </activity>

        <activity
                android:name="com.example.hello.PageCalc"
                android:label="@string/app_name"
                android:parentActivityName="com.example.hello.MyActivity" >
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.hello.MyActivity" />
        </activity>

    </application>
</manifest>
